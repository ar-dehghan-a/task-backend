{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "consistent-return": "off",
    "no-unused-vars": "off",
    "no-param-reassign": "off"
    // "no-console": "off"
  }
}
